                                        "children": [
                                            {
                                                "data": {
                                                    "id": "c4d5bb97hn40",
                                                    "created": 1595420351360,
                                                    "text": "mvn archetype:generate \"-DgroupId=com.companyname.bank\" \"-DartifactId=consumerBanking\" \"-DarchetypeArtifactId=maven-archetype-quickstart\" \"-DinteractiveMode=false\""
                                                },
                                                "children": []
                                            },
                                            {
                                                "data": {
