                            "expandState": "collapse"
                        },
                        "children": [
                            {
                                "data": {
                                    "id": "c3lxs9kpwnc0",
                                    "created": 1592656992184,
                                    "text": "一个开源的轻量级的 Java 开发框架"
                                },
                                "children": [
                                    {
                                        "data": {
                                            "id": "c3lxunrhjr40",
                                            "created": 1592657179797,
                                            "text": "是分层的 Java SE/EE full-stack 轻量级开源框架"
                                        },
                                        "children": []
                                    }
                                ]
                            },
                            {
                                "data": {
                                    "id": "c3lxsmbhyg00",
                                    "created": 1592657019925,
                                    "text": "两大核心"
                                },
                                "children": [
                                    {
                                        "data": {
                                            "id": "c3lxssl2jtc0",
